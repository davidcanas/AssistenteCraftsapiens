{
  "compilerOptions": {
    "target": "es2021",
    "outDir": "./dist",
    "rootDir": "./src",
    "strictNullChecks": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": ["ES2023.Array"],                                   
    "module": "commonjs",
    "typeRoots": [
      "src/typings",
      "node_modules/@typings"
    ],
    "strictPropertyInitialization": false,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true 
  }
}